{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ClassMaster (Edge, Release, Port 5000)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "edge",
                "--release",
                "--web-port",
                "5000",
                "--web-hostname",
                "localhost"
            ],
            "flutterMode": "release",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "flutterClean"
        }
    ]
}